speedDecrease.Name = "speedDecrease"  -- Renomeado de speed_2 para speedDecrease
speedDecrease.Parent = backgrondfly
speedDecrease.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
speedDecrease.BorderColor3 = Color3.fromRGB(250, 221, 0)
speedDecrease.BorderSizePixel = 0
speedDecrease.Position = UDim2.new(0.749406576, 0, 0.381521642, 0)
speedDecrease.Size = UDim2.new(0, 31, 0, 31)
speedDecrease.Font = Enum.Font.FredokaOne
speedDecrease.Text = "-"
