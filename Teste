logoSyn.Image = "rbxassetid://14862323455"

bordas.Name = "bordas"
bordas.Parent = backgrondfly

degrad.Color = ColorSequence.new{ColorSequenceKeypoint.new(0.00, Color3.fromRGB(2, 2, 2)), ColorSequenceKeypoint.new(0.55, Color3.fromRGB(60, 3, 66)), ColorSequenceKeypoint.new(1.00, Color3.fromRGB(67, 2, 75))}
degrad.Name = "degrad"
degrad.Parent = backgrondfly

