local speedUp = Instance.new("TextButton")  -- Renomeado de up para speedUp
local UICorner_2 = Instance.new("UICorner")
local UIGradient_2 = Instance.new("UIGradient")
local speedDown = Instance.new("TextButton")  -- Renomeado de down para speedDown
local UICorner_3 = Instance.new("UICorner")
local UIGradient_3 = Instance.new("UIGradient")
local speedIncrease = Instance.new("TextButton")  -- Renomeado de speed para speedIncrease
local UICorner_4 = Instance.new("UICorner")
local UIGradient_4 = Instance.new("UIGradient")
local speedDecrease = Instance.new("TextButton")  -- Renomeado de speed_2 para speedDecrease
local UICorner_5 = Instance.new("UICorner")
local UIGradient_5 = Instance.new("UIGradient")

-- Properties:

flyv1.Name = "flyv1"
